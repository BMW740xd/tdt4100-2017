//Lage ny liste med innholdjava.util.List<String> list = new java.util.ArrayList<String>(java.util.Arrays.asList("hei", "paa", "deg", "hva", "skjer"));System.out.println(list);  // printer listen//Diverse listeoperasjonerSystem.out.println(list.size());  // LengdeSystem.out.println(list.get(0));  // Første elementSystem.out.println(list.get(list.size()-1));  // Siste elementSystem.out.println(list.subList(3, list.size()));  // subliste med elementer fra indeks 3 ut listaSystem.out.println(list.subList(0, 2));  // Første 2 elementerSystem.out.println(list.subList(list.size()-2, list.size()));  //Siste 2 elementerlist.remove(0);  //fjerner element på index 0, returnerer true eller falseSystem.out.println(list);list.remove("paa");  // fjerner elementet "paa", returnerer true hvis har gått bra, ellers falseSystem.out.println(list);list.add("java"); // Legger til elementet "java", returnerer true hvis har gått bra, ellers falseSystem.out.println(list);list.set(0, "meg");  // Setter "meg" på index 0System.out.println(list); System.out.println(list.contains("meg"));  // Sjekker om listen inneholder elementet "meg"System.out.println(list.indexOf("java"));  // Returnerer indeksen til elementet "java"